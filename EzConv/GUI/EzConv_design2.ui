   <string notr="true">/* Общие настройки */
QWidget {
    background-color: white;
    color: #333333;
    font-family: 'Arial', sans-serif;
}

/* Кнопки */
QPushButton {
    background-color: #FFC107;
    color: #333333;
    border: 2px solid #FFA000;
    border-radius: 5px;
}

QPushButton:hover {
    background-color: #FFD54F;
}

QPushButton:pressed {
    background-color: #FFA000;
}

/* Текстовые поля */
QLineEdit, QTextEdit {
    background-color: #FFFFFF;
    border: 1px solid #FFC107;
    border-radius: 3px;
    padding: 5px;
}

QLineEdit:focus, QTextEdit:focus {
    border: 2px solid #FF9800;
}

/* Заголовки */
QLabel {
    color: #FF6F00;
    font-weight: bold;
	font: 11px
}

/* Меню */
QMenuBar {
    background-color: white;
    color: #333333;
}

QMenuBar::item {
    spacing: 5px;
    padding: 3px 10px;
}

QMenuBar::item:selected {
    background-color: #FFC107;
}

/* Прогресс-бар */
QProgressBar {
    border: 2px solid #FFC107;
    border-radius: 5px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: #FFC107;
}

/* Скроллбар */
QScrollBar:vertical {
    background-color: #FFF9C4;
    width: 15px;
}

QScrollBar::handle:vertical {
    background-color: #FFC107;
    min-height: 20px;
}

/* Чекбоксы и радио-кнопки */
QCheckBox, QRadioButton {
    spacing: 5px;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 18px;
    height: 18px;
}

QCheckBox::indicator:checked {
    background-color: #FFC107;
}

/* Комбо-бокс */
QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #FFC107;
    border-radius: 3px;
    padding: 5px;
}

QComboBox::drop-down {
    background-color: #FFC107;
}

QMessageBox{
	background-color: #FFFDE7;
    color: #333333;
    font-family: 'Arial', sans-serif;
}</string>
