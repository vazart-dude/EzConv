   <string notr="true">/* Основные настройки */
QWidget {
    background-color: #FFFBEA;
    color: #2D3748;
    font-family: 'Segoe UI', 'Roboto', sans-serif;
}
/* Меню с расширенной анимацией */
QMenuBar {
    background-color: #FFF3CD;
    color: #2D3748;
    spacing: 5px;
}

QMenuBar::item {
    background-color: transparent;
    padding: 5px 15px;
    border-radius: 4px;
    transition: all 0.3s ease;
}

QMenuBar::item:hover {
    background-color: rgba(255, 230, 150, 0.6);
    color: #664D03;
    transform: scale(1.05);
}

QMenuBar::item:pressed {
    background-color: rgba(255, 215, 0, 0.8);
    color: #333;
    transform: scale(0.95);
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2);
}

QMenuBar::item:selected {
    background-color: rgba(255, 230, 150, 0.8);
    color: #664D03;
}

/* Выпадающее меню */
QMenu {
    background-color: #FFFFFF;
    border: 1px solid #FFE5B4;
    border-radius: 5px;
    padding: 5px;
    min-width: 150px;
}

QMenu::item {
    background-color: transparent;
    padding: 5px 20px;
    margin: 2px;
    border-radius: 3px;
    transition: all 0.3s ease;
}

QMenu::item:hover {
    background-color: #FFF3CD;
    color: #664D03;
}

QMenu::item:selected {
    background-color: #FFE69C;
    color: #333;
}

QMenu::separator {
    height: 1px;
    background-color: #FFE5B4;
    margin: 5px 0;
}
/* Кнопки */
QPushButton {
    background-color: #FFF3CD;
    color: #664D03;
    border: 1px solid #FFECB5;
    border-radius: 4px;
    font-weight: 500;
}

QPushButton:hover {
    background-color: #FFE69C;
    border-color: #FFC107;
}

QPushButton:pressed {
    background-color: #FFD700;
    border-color: #FFA500;
}

/* Текстовые поля */
QLineEdit, QTextEdit {
    background-color: #FFFFFF;
    border: 1px solid #FFE5B4;
    border-radius: 3px;
    padding: 5px;
    selection-background-color: #FFC107;
}

QLineEdit:focus, QTextEdit:focus {
    border: 1px solid #FFA500;
    box-shadow: 0 0 5px rgba(255, 165, 0, 0.3);
}

/* Заголовки */
QLabel {
    color: #664D03;
    font-weight: 600;
}

/* Меню */
QMenuBar {
    background-color: #FFF3CD;
    color: #2D3748;
}

QMenuBar::item {
    spacing: 5px;
    padding: 4px 12px;
    background-color: transparent;
}

QMenuBar::item:selected {
    background-color: #FFE69C;
}

/* Прогресс-бар */
QProgressBar {
    border: 1px solid #FFE5B4;
    border-radius: 4px;
    text-align: center;
    color: #664D03;
}

QProgressBar::chunk {
    background-color: #FFC107;
    width: 10px;
    margin: 0.5px;
}

/* Скроллбар */
QScrollBar:vertical {
    background-color: #FFF3CD;
    width: 12px;
}

QScrollBar::handle:vertical {
    background-color: #FFD700;
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background-color: #FFC107;
}

/* Чекбоксы и радио-кнопки */
QCheckBox, QRadioButton {
    spacing: 5px;
    color: #2D3748;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #FFE5B4;
    border-radius: 3px;
}

QCheckBox::indicator:checked {
    background-color: #FFC107;
    image: url(path/to/check-icon.png); /* Опционально */
}

/* Комбо-бокс */
QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #FFE5B4;
    border-radius: 3px;
    padding: 5px;
    transition: all 0.3s ease;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border-left-width: 1px;
    border-left-color: #FFE5B4;
    border-left-style: solid;
}

QComboBox::down-arrow {
	image: url(down-arrow.png);
	subcontrol-position: right;
	height: 15px;
	width: 15px;
}

QComboBox:hover {
    transform: scale(1.02);
}

QComboBox:pressed {
    transform: scale(0.98);
}

/* Вкладки */
QTabWidget::pane {
    border: 1px solid #FFE5B4;
    background-color: #FFFFFF;
}

QTabBar::tab {
    background-color: #FFF3CD;
    color: #664D03;
    padding: 5px 15px;
    margin-right: 2px;
    border: 1px solid #FFE5B4;
    border-bottom: none;
}

QTabBar::tab:selected {
    background-color: #FFFFFF;
    border-bottom: 2px solid #FFC107;
}</string>
